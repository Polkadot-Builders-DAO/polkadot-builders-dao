{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "DEPLOYER_MOONBEAM",
    "DEPLOYER_MOONBASE",
    "MOONSCAN_API_KEY",
    "VITE_CHAIN_ID",
    "FOUNDERS",
    "DAO",
    "NODE_ENV"
  ],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "nft:deploy:moonbase": {},
    "nft:deploy:moonbeam": {},
    "test": {},
    "test:watch": {},
    "compile": { "cache": false }
  }
}
